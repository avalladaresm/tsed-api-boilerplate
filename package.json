{
	"name": "tsed-api-boilerplate",
	"version": "0.0.1",
	"description": "",
	"scripts": {
		"build": "npx tsc --project tsconfig.compile.json",
		"build:w": "tsc --project tsconfig.compile.json -w",
		"start": "tsnd --inspect --ignore-watch node_modules --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
		"start:prod": "cross-env NODE_ENV=production node dist/index.js",
		"test": "npm run test:lint && npm run test:coverage",
		"test:unit": "cross-env NODE_ENV=test jest",
		"test:coverage": "npm run test:unit",
		"test:lint": "eslint src --ext .ts",
		"test:lint:fix": "eslint src --ext .ts --fix",
		"prettier": "prettier '{src,test}/**/*.ts' --write",
		"seed:config": "ts-node ./node_modules/typeorm-seeding/dist/cli.js config",
		"seed:run": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed",
		"typeorm": "tsed typeorm",
		"typeorm:runmig": "ts-node --transpile-only ./node_modules/typeorm/cli.js --config ./src/ormconfig.ts"
	},
	"dependencies": {
		"@tsed/ajv": "^7.0.0-beta.3",
		"@tsed/common": "^7.0.0-beta.3",
		"@tsed/core": "^7.0.0-beta.3",
		"@tsed/di": "^7.0.0-beta.3",
		"@tsed/exceptions": "^7.0.0-beta.3",
		"@tsed/json-mapper": "^7.0.0-beta.3",
		"@tsed/platform-express": "^7.0.0-beta.3",
		"@tsed/schema": "^7.0.0-beta.3",
		"@tsed/socketio": "^7.0.0-beta.3",
		"@tsed/swagger": "^7.0.0-beta.3",
		"@tsed/typeorm": "^7.0.0-beta.3",
		"ajv": "^8.8.2",
		"bcrypt": "^5.0.1",
		"body-parser": "^1.19.1",
		"compression": "^1.7.4",
		"cookie-parser": "^1.4.6",
		"cors": "^2.8.5",
		"cross-env": "^7.0.3",
		"dotenv": "^10.0.0",
		"express": "^4.17.2",
		"jsonwebtoken": "^8.5.1",
		"method-override": "^3.0.0",
		"mssql": "^7.3.0",
		"nodemailer": "^6.7.2",
		"nodemailer-mailgun-transport": "^2.1.3",
		"otplib": "^12.0.1",
		"socket.io": "^4.4.0",
		"typeorm": "^0.2.41"
	},
	"devDependencies": {
		"@tsed/cli-plugin-eslint": "3.16.0",
		"@tsed/cli-plugin-jest": "3.16.0",
		"@tsed/cli-plugin-typeorm": "3.16.0",
		"@types/bcrypt": "^5.0.0",
		"@types/compression": "^1.7.2",
		"@types/cookie-parser": "^1.4.2",
		"@types/cors": "^2.8.12",
		"@types/express": "^4.17.13",
		"@types/faker": "^5.5.9",
		"@types/jest": "^27.4.0",
		"@types/jsonwebtoken": "^8.5.6",
		"@types/method-override": "^0.0.32",
		"@types/multer": "^1.4.7",
		"@types/node": "^17.0.5",
		"@types/nodemailer": "^6.4.4",
		"@types/supertest": "^2.0.11",
		"@typescript-eslint/eslint-plugin": "^5.8.1",
		"@typescript-eslint/parser": "^5.8.1",
		"eslint": "^8.6.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^4.0.0",
		"husky": "^7.0.4",
		"jest": "^27.4.5",
		"lint-staged": "^12.1.4",
		"prettier": "^2.5.1",
		"supertest": "^6.1.6",
		"ts-jest": "^27.1.2",
		"ts-node": "^10.4.0",
		"ts-node-dev": "^1.1.8",
		"tsconfig-paths": "^3.12.0",
		"tslib": "^2.3.1",
		"typeorm-seeding": "^1.6.1",
		"typescript": "^4.5.4"
	},
	"tsed": {
		"packageManager": "npm",
		"convention": "default",
		"architecture": "default"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged",
			"post-commit": "git update-index --again"
		}
	}
}
